class Solution {
public:
    int numDifferentIntegers(string word) {
        set<string> s;
        string str="";
        for(int i=0;i<word.size();i++)
        {
            if(word[i]>='0' && word[i]<='9')
            {
                while(i<word.size() && word[i]>='0' && word[i]<='9')
                str+=word[i++];
                int j=0;
                while(str[j]=='0')
                j++;
                str.erase(str.begin(),str.begin()+j);
                s.insert(str);
                str="";
            }
        }
        return s.size();
    }
};
