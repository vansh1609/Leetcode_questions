class Solution {
public:
    int arraySign(vector<int>& nums) {
        double p=1;
        for(int i=0;i<nums.size();i++)
        {
            if(nums[i]==0)
                return 0;
        }
        for(int i=0;i<nums.size();i++)
            p*=nums[i];
        if(p<0)
            return -1;
        else if(p==0)
            return 0;
        else
            return 1;
        
    }
};
